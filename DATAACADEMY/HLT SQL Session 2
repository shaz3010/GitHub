create table Car_details(car_id integer, make varchar(100), model varchar(100), reg varchar(100), colour varchar(100));
insert into Car_details(car_id, make, model, reg, colour) values(1, "BMW", "3 series", "ew45 uyt", "black");
(2, "Fiat", "Punto", "ew96 qaz", "Pink");
(3, "Ford", "Sprinter", "hg45 cvs", "yellow");
