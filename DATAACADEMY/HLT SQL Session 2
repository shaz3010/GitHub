(5, "Mazda", "Don", "op12 qat", "silver");
(6, "Daewoo", "Basanti", "ks19 CFV", "blue");
(7, "Audi", "TT", "pl87 bmj", "green");
(8, "VW", "Passat", "xc16 sdf", "brown");
(9, "VW", "Touran", "pl15 mnb", "purple");
(10, "Volvo", "Dune", "vg20 sjq", "magenta");
update Car_details
set colour = "pink"
where car_id = 1;
delete from Car_details
where car_id = "4";
